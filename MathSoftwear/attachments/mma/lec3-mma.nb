    RowBox[{
    "20", "\:7684\:81ea\:7136\:6570\:662f\:5426\:662f\:7d20\:6570"}]}], 
   "*)"}]}], "\n", 
 RowBox[{"IsPrime", "/@", 
  RowBox[{"Range", "[", "20", "]"}]}], "\n", 
 RowBox[{
  RowBox[{"PrimeSum", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"s", "=", "0"}], "}"}], ",", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"IsPrime", "[", "i", "]"}], ",", 
         RowBox[{"s", "+=", "i"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "n"}], "}"}]}], "]"}], ";", "s"}]}], 
   "]"}]}], "\n", 
