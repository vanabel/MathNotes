   "*)"}]}], "\n", 
 RowBox[{"IsPrime", "/@", 
  RowBox[{"Range", "[", "20", "]"}]}], "\n", 
 RowBox[{
  RowBox[{"PrimeSum", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"s", "=", "0"}], "}"}], ",", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"IsPrime", "[", "i", "]"}], ",", 
         RowBox[{"s", "+=", "i"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "n"}], "}"}]}], "]"}], ";", "s"}]}], 
   "]"}]}], "\n", 
