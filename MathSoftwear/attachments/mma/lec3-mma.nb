 RowBox[{
  RowBox[{
   RowBox[{"IsPrime", "[", "n_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"max", "=", 
        RowBox[{
         RowBox[{"Floor", "[", 
          RowBox[{"Sqrt", "[", "n", "]"}], "]"}], "+", "1"}]}], ",", 
       RowBox[{"i", "=", "2"}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"i", "<", "max"}], "&&", 
         RowBox[{
          RowBox[{"Mod", "[", 
           RowBox[{"n", ",", "i"}], "]"}], "!=", "0"}]}], ",", 
        RowBox[{"i", "++"}]}], "]"}], ";", 
      RowBox[{
       RowBox[{"n", ">=", "2"}], "&&", 
       RowBox[{"i", ">=", "max"}]}]}]}], "]"}]}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"\:8f93\:51fa1", "-", 
    RowBox[{"20", "\:7684\:6240\:6709\:81ea\:7136\:6570"}]}], "*)"}]}], "\n", 

 RowBox[{
