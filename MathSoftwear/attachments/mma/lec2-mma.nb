           RowBox[{"difference", "==", "0"}], ",", 
           RowBox[{
            RowBox[{"Print", "[", 
             RowBox[{
             "\"\<Congratulations! You've guessed the right number: \>\"", 
              ",", "target", ",", "\"\<!\>\""}], "]"}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"(*", 
             RowBox[{
             "Exit", " ", "the", " ", "loop", " ", "when", " ", "guessed", 
              " ", "correctly"}], "*)"}], 
            RowBox[{"Break", "[", "]"}]}], ",", 
           RowBox[{"difference", "<=", "5"}], ",", 
           RowBox[{
           "Print", "[", "\"\<Very close! You're within 5.\>\"", "]"}], ",", 
           RowBox[{"difference", "<=", "15"}], ",", 
           RowBox[{"Print", "[", "\"\<Close! You're within 15.\>\"", "]"}], 
           ",", 
           RowBox[{"difference", "<=", "30"}], ",", 
           RowBox[{
           "Print", "[", "\"\<Not too far! You're within 30.\>\"", "]"}], ",",
            "True", ",", 
           RowBox[{"Print", "[", "\"\<Way off! Try again!\>\"", "]"}]}], 
          "]"}], ";"}]}], "]"}]}]}], "]"}]}], "\n", 
  RowBox[{"(*", 
